<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <columns name="product_columns">
        <column name="qty"
                class="Magento\InventoryCatalog\Ui\Component\Listing\Column\SourceItems"
                component="Magento_InventoryCatalog/js/product/grid/cell/source-items"
                sortOrder="75">
            <settings>
                <filter>false</filter>
                <sortable>false</sortable>
                <label translate="true">Quantity</label>
            </settings>
        </column>
        <column name="stocks"
                class="Magento\InventoryCatalog\Ui\Component\Listing\Column\Stocks"
                component="Magento_InventoryCatalog/js/product/grid/cell/stocks"
                sortOrder="85">
            <settings>
                <filter>false</filter>
                <sortable>false</sortable>
                <label translate="true">Quantity per Stock</label>
            </settings>
        </column>
    </columns>
</listing>
